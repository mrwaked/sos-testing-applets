{
  "name": "@kral/stream-tester",
  "version": "1.0.0",
  "main": "dist/index.html",
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "upload": "sos applet upload",
    "clean": "npx rimraf dist",
    "escheck": "es-check --module es5 dist/*.js",
    "postbuild": "npm run escheck",
    "start": "webpack serve --mode development",
    "build": "webpack --mode production && npm run escheck",
    "connect": "webpack --watch"
  },
  "files": [
    "dist"
  ],
  "description": "signageOS applet",
  "repository": {},
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@signageos/front-applet": "^6.7.0",
    "@signageos/front-display": "^14.7.1",
    "@signageos/webpack-plugin": "^1.0.5",
    "babel-loader": "^8.4.1",
    "css-loader": "^6.11.0",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.2"
  },
  "sos": {
    "appletUid": "6d2df0fac1977bd56bdcdf2cd90d934828018d02f9c6a3d3cc",
    "appletVersion": "1.0.0",
    "config": [
      {
        "name": "uri",
        "valueType": "string",
        "description": "URL to stream",
        "placeholder": "https://www.rmp-streaming.com/media/bbb-360p.mp4",
        "mandatory": true
      },
      {
        "name": "protocolType",
        "valueType": "string",
        "description": "Type of protocol - HTTP, RTMP, RTSP, HLS",
        "placeholder": "HTTP",
        "mandatory": true
      }
    ]
  }
}
